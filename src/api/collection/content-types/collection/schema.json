{
  "kind": "collectionType",
  "collectionName": "collections",
  "info": {
    "singularName": "collection",
    "pluralName": "collections",
    "displayName": "Collection"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "added": {
      "type": "date"
    },
    "picture": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "skins": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::skin.skin",
      "mappedBy": "collection"
    }
  }
}
